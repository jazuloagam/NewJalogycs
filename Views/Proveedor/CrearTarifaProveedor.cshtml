                                    <el-row>
                                        <div>
                                            <div class="ln_solid"></div>
                                            <div>
                                                <h1 class="section_title"><i class="el-icon-user"></i><b>Cargar conceptos</b></h1>
                                            </div>
                                            <div class="ln_solid"></div>
                                            <el-col :span="24">
                                                <tablaedit :columnas="columnatablamensajeriatarifa" :datos="datotablamensajeriatarifa" :key="componentKey" :datofila="datocolumnamensajeriatarifa" size="mini" :idarraycoldatoseleccion="columnaidcambiodatoseleccion" v-on:change="changetablaedit" v-on:changeeliminacion="changetablaediteliminacion"  :adicionaropcion="validarcreartarifa" v-on:validacionespaso="validacionespaso"></tablaedit>
